{
  "name": "@tkssharma/nestjs-pkg",
  "version": "1.0.4",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "release": "npm publish",
    "build": "tsc",
    "check-engines": "node ./node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "clean": "rimraf build && rimraf coverage",
    "prebuild": "npm run clean && npm run check-engines ",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test:ci",
    "test:ci": "jest --ci --collectCoverage --maxWorkers 2 --passWithNoTests"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "files": [
    "package.json",
    "build",
    "README.md"
  ],
  "devDependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.0.0",
    "@nestjs/microservices": "9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "fbjs-scripts": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "uuid": "^8.3.2"
  }
}
